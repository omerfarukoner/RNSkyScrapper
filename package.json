{
  "name": "rnskyscrapper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:simulator": "react-native run-ios --simulator=\"iPhone 16\"",
    "ios:sim:current": "react-native run-ios --simulator=\"$(xcrun simctl list devices | grep Booted | head -1 | sed 's/.*(//' | sed 's/).*//')\"",
    "ios:device": "react-native run-ios --device",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf build/ && xcodebuild clean && cd ..",
    "clean:all": "yarn clean:android && yarn clean:ios",
    "reset:android": "yarn clean:android",
    "reset:ios": "yarn clean:ios && cd ios && rm -rf build/ && rm -rf Pods/ && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod install && cd ..",
    "reset:all": "yarn reset:android && yarn reset:ios"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.3.1",
    "axios": "^1.9.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
